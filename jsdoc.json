{
    "source": {
        "include": [
            "./src/js/",
            "./README.md"
        ],
        "includePattern": ".js$"
    },
    "opts": {
        "destination": "./docs",
        "template": "./node_modules/docdash"
    },
    "templates": {
        "default": {
            "outputSourceFiles": false,
            "useLongnameInNav": true
        },
        "useLongnameInNav": true
    },
    "plugins": ["plugins/markdown"]
}
