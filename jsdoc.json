{
    "source": {
        "include": [
            "./src/js/",
            "./dist/jseasy.min.js",
            "./README.md"
        ],
        "includePattern": ".js$"
    },
    "opts": {
        "destination": "./docs/0.1.0",
        "template": "./node_modules/docdash"
    },
    "templates": {
        "default": {
            "outputSourceFiles": true,
            "useLongnameInNav": true,
            "staticFiles": {
                "paths": ["./dist/jseasy.min.js"]
            },
            "layoutFile": "./src/js/jsdocs/layout.tmpl"
        },
        "useLongnameInNav": true
    },
    "plugins": ["plugins/markdown"],
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
}
